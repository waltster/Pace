{
  "compilerOptions": {
      "module": "esnext",
      "outDir": "./dist",
      "rootDir": "./src",
      "moduleResolution": "node",
      "target": "es2019",
      "lib": ["es2020"],
      "sourceMap": false,
      "noImplicitAny": true,
      "esModuleInterop": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
